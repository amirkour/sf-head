{
  "name": "sf-head",
  "version": "1.0.0",
  "description": "Parent meta repo for sf-head",
  "scripts": {
    "init:clone:admin": "git clone https://github.com/amirkour/sf-head-admin.git projects/sf-head-admin",
    "init:clone:web": "git clone https://github.com/amirkour/sf-head-web.git projects/sf-head-web",
    "init:admin": "npm run init:clone:admin && cd projects/sf-head-admin && npm i",
    "init:web": "npm run init:clone:web && cd projects/sf-head-web && npm i",
    "init": "npm run init:admin && npm run init:web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amirkour/sf-head.git"
  },
  "author": "Amir Kouretchian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amirkour/sf-head/issues"
  },
  "homepage": "https://github.com/amirkour/sf-head#readme"
}
